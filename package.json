{
  "name": "youtube-detox",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm -r --parallel run dev",
    "dev:ext": "pnpm --filter @yt-detox/extension dev",
    "dev:api": "pnpm --filter @yt-detox/backend dev",
    "build": "pnpm -r run build",
    "build:ext": "pnpm --filter @yt-detox/extension build",
    "lint": "pnpm -r run lint",
    "typecheck": "pnpm -r run typecheck"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
